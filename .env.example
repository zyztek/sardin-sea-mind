# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Production Environment
VITE_APP_ENV=production
VITE_APP_VERSION=1.0.0

# Maritime System Configuration
VITE_MARITIME_SYSTEM_NAME=SARDIN-AI
VITE_DEFAULT_POSITION_LAT=42.3601
VITE_DEFAULT_POSITION_LNG=-71.0589

# API Endpoints (Optional)
VITE_WEATHER_API_KEY=your_weather_api_key
VITE_AIS_API_ENDPOINT=https://api.vesselfinder.com
VITE_MARINE_TRAFFIC_API_KEY=your_marine_traffic_key

# Monitoring & Analytics
VITE_ERROR_TRACKING_DSN=your_sentry_dsn
VITE_ANALYTICS_ID=your_analytics_id

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PWA=true
VITE_ENABLE_REAL_TIME_SYNC=true
VITE_DEBUG_MODE=false